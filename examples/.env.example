DEBUG <bool> = true
HOST <str> = "localhost"
PORT <int> = 8000
URL <str> = ${HOST}:${PORT}
SCHEMA <str> = "http"
DB_NAME <str> = "my_database"
DATABASE_URL <str> = ${SCHEMA}://${HOST}:${PORT}/${DB_NAME}


# Simple types
NAME=John Doe
AGE<int>=25
HEIGHT<float>=5.9
IS_ACTIVE<bool>=true

# Nested list types
NUMBERS<list<int>>=1,2,3,4,5
SCORES<list<float>>=95.5,87.2,92.1
TAGS<list<str>>=python,dev,backend

# Nested dict types
CONFIG<dict<str, int>>=timeout=30,retries=3,port=8080
SETTINGS<dict<str, str>>=host=localhost,db=myapp,env=dev

# Json-like structures
JSON_DATA<json>={"name": "John", "age": 30, "city": "New York"}

# Nested casting not supported
# # Deeply nested types
# MATRIX<list<list<int>>>=[[1,2,3],[4,5,6],[7,8,9]]
# USERS<list<dict<str, str>>>={"name":"John","role":"admin"},{"name":"Jane","role":"user"}

# # Complex nested structures
# COMPLEX<dict<str, list<int>>>=group1=[1,2,3],group2=[4,5,6]
# NESTED_CONFIG<dict<str, dict<str, int>>>={"db":{"port":5432,"timeout":30},"cache":{"ttl":3600,"size":1000}}

# # Variable resolution with casting
BASE_NUMBERS<list<int>>=1,2,3
EXTENDED_NUMBERS<list<int>>=${BASE_NUMBERS},4,5,6

